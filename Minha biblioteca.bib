@article{pekerti_cultural_2017,
	title = {Do {Cultural} and {Generational} {Cohorts} {Matter} to {Ideologies} and {Consumer} {Ethics}? {A} {Comparative} {Study} of {Australians}, {Indonesians}, and {Indonesian} {Migrants} in {Australia}},
