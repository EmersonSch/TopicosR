@article{howe_next_2007,
	title = {The next 20 years: how customer and workforce attitudes will evolve.},
	volume = {85 7-8},
	abstract = {Projetos de negócios com horizontes de tempo muito longos - como aqueles que envolvem P\&D de produtos, design de local de trabalho e planejamento de remuneração total - precisam lidar com uma questão crucial: Quais serão as necessidades, demandas e desejos dos consumidores e funcionários daqui a décadas? Se você acha que a resposta é “Só mais do mesmo”, você está em uma surpresa. Howe e Strauss, os autores de Gerações, A Quarta Turna, Millennials em ascensão e outros livros, estudaram as diferenças entre as gerações há cerca de 30 anos. Sua extensa pesquisa revelou um padrão fascinante — um tão forte que suporta uma medida de previsibilidade. Com base no precedente histórico, dizem eles, podemos prever como as gerações que estão vivas hoje pensarão e agirão nas próximas décadas. Três dessas gerações ainda serão forças vitais na sociedade americana daqui a 20 anos: Boomers, Geração X e Millennials. Suas atitudes e comportamentos terão efeitos profundos na economia, no local de trabalho e nas instituições sociais em geral. Por exemplo, à medida que os Boomers envelhecidos evitam a medicina de alta tecnologia em favor do autocuidado holístico, alimentos naturais e técnicas de cura mente-corpo, alguns hospitais estão abrindo novas asas com medicina alternativa e aconselhamento espiritual. O general Xers, tendo crescido em uma era de escolas e casamentos fracassados, permanecerá alienado, descontente e pragmático à medida que entrarem na meia-idade. Já a maior geração empreendedora da história dos EUA, eles serão altamente eficazes para impulsionar a inovação, a eficiência e a personalização em massa. Em contraste, os jovens adultos Millennials favorecerão o trabalho em equipe, relacionamentos familiares próximos, segurança no trabalho e uma cultura popular sem graça. Seu empoderamento digital sem precedentes e talento para organização criarão uma potência política e podem até revitalizar o movimento sindical.},
	journal = {Harvard business review},
	author = {Howe, N and Strauss, William},
	year = {2007},
	pages = {41--52,191},
}

